{
    "manifest_version": 3,
    "name": "Simple Tab Manager",
    "version": "1.0",
    "description": "Manage your tabs efficiently.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "assets/icon.png",
        "48": "assets/icon.png",
        "128": "assets/icon.png"
    }
    },
    "permissions": ["tabs"]
}
  